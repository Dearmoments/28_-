{
  "output": {
    "filePath": "repomix-output.xml",
    "style": "xml",
    "removeComments": false,
    "removeEmptyLines": false
  },
  "include": [
    "**/*.v",
    "**/*.vh",
    "**/*.c",
    "**/*.h",
    "**/*.m",
    "**/*.md",
    "**/*.txt"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "**/db/**",
      "**/incremental_db/**",
      "**/output_files/**",
      "**/simulation/**",
      "**/greybox_tmp/**",
      "**/*.vo",
      "**/*.vho",
      "**/*.sdo",
      "**/*.sft",
      "**/*.bak",
      "**/*.rpt",
      "**/*.summary",
      "**/*.smsg",
      "**/*.done",
      "**/*.qarlog",
      "**/*.qws",
      "**/*.sof",
      "**/*.pof",
      "**/*.jdi",
      "**/*.pin",
      "**/*.csv",

      "**/EMWIN/Demo/**",
      "**/EMWIN/**",
      "**/EMWIN/STemWinLibrary532/**",
      "**/EMWIN/STemWin/inc/**",
      "**/FWLIB/**",
      "**/CORE/**",
      "**/UCOSIII/uC-CPU/**",
      "**/UCOSIII/uC-LIB/**",
      "**/UCOSIII/uCOS-III/**",
      "**/SYSTEM/**",

      "**/USER/stm32f4xx.h",
      "**/USER/system_stm32f4xx.c",
      "**/USER/stm32f4xx_conf.h",
      "**/USER/stm32f4xx_it.c",
      "**/USER/stm32f4xx_it.h",
      "**/HARDWARE/LCD/FONT.h",

      "**/OBJ/**",
      "**/Listings/**",
      "**/Objects/**",
      "**/DebugConfig/**",
      "**/RTE/**",
      "**/*.uvguix.*",
      "**/*.uvoptx",
      "**/JLinkSettings.ini",
      "**/EventRecorderStub.scvd",
      "**/*.map",
      "**/*.lst",
      "**/*.axf",
      "**/*.elf",
      "**/*.hex",
      "**/*.bin",

      "**/*.SchDoc",
      "**/*.PcbDoc",
      "**/*.SchLib",
      "**/*.PcbLib",
      "**/*.vsdx",
      "**/*.PrjPcb",
      "**/*.PrjPcbStructure",
      "**/History/**",
      "**/__Previews/**",
      "**/Project Logs*/**",
      "**/Project Outputs*/**",

      "**/*.asv",
      "**/*.m~",

      "**/Debug/**",
      "**/Release/**",
      "**/.vs/**",
      "**/*.user",
      "**/*.suo",
      "**/*.sln",
      "**/*.vcxproj",
      "**/*.vcxproj.filters",
      "**/图片/**",
      "**/图片与视频/**",
      "**/参考资料/**",
      "**/原理图/**",
      "**/题目/**",
      "**/方案说明/**",
      "**/文档/**",

      "**/repomix-output.xml",
      "**/keilkilll.bat",
      "**/QuartusII工程清理.bat"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
